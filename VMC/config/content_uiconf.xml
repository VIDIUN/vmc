<root>
	<locale>
		<path>locale/{locale}/content_{locale}_ResourceModule.swf</path>
		<language>en_US</language>
	</locale>
	<uiconf>
		<vdp>
			<moderation>@@ModerationUIConfID@@</moderation>
			<drilldown>@@VdpDrilldownForContentUIConfID@@</drilldown>
			<drilldownAds>@@VdpAdsTabUIConfID@@</drilldownAds>
			<captureThumbnail>@@VdpCaptureThumbnailForContentUIConfID@@</captureThumbnail>
		</vdp>
		<metadata>@@MetadataUIConfID@@</metadata>
		<vclip>
			<ads>@@VClipAdsDrillDownUIConfID@@</ads>
		</vclip>
		<entryDrilldown>
			<panels>
				<panel id="entryMetaData" url="/modules/ved/EntryData.swf" styleName="metaDataHbox" index="0" />
				<panel id="entryThumbnails" url="/modules/ved/EntryThumbnails.swf" index="1" />
				<panel id="entryAcp" url="/modules/ved/EntryAccessControl.swf" styleName="accessControlVbox" index="2" />
				<panel id="entrySch" url="/modules/ved/EntryScheduling.swf" index="3" />
				<panel id="entryAssets" url="/modules/ved/EntryFlavors.swf" styleName="flavorsVbox" index="4" />
				<panel id="contentTab" url="/modules/ved/EntryContent.swf" index="5" />
				<panel id="liveStream" url="/modules/ved/EntryLiveStreaming.swf" index="6" />
				<!-- panel id="customData" url="/modules/ved/EntryCustomData.swf" index="7" /-->
				<panel id="entryDistribution" url="/modules/ved/EntryDistribution.swf" index="8" />
				<panel id="entryCaptions" url="/modules/ved/EntryCaptions2.swf" index="9" />
				<panel id="ads" url="/modules/ved/EntryAds.swf" index="10" />
				<panel id="entryRelated" url="/modules/ved/EntryRelated.swf" index="11" />
				<panel id="entryClips" url="/modules/ved/EntryClips.swf" index="12" />
				<panel id="entryUsers" url="/modules/ved/EntryUsers.swf" index="13" />
			</panels>
		</entryDrilldown>
	</uiconf>
	<!-- sampleRate: category status sample rate (in ms) 
		singleLevelMax: max categories to show in tree per single level-->
	<categories sampleRate="30000" singleLevelMax="1500" />
	<!-- bulkupload sample files -->
	<bulkupload>/content/docs/vidiun_batch_upload_eagle.zip</bulkupload>
	<!-- cueopints sample -->
	<cuepoints>/content/docs/xml/adCuePointSampleFile.xml</cuepoints>
	<!-- file types -->
	<filters>
		<filter name="video_files" ext="*.flv;*.asf;*.qt;*.mov;*.mpg;*.avi;*.wmv;*.mp4;*.3gp;*.f4v;*.m4v;*.mpeg;*.mxf;*.rm;*.rv;*.rmvb;*.ts;*.ogg;*.ogv;*.vob;*.webm;*.mts;*.arf;*.mkv"/>
		<filter name="audio_files" ext="*.flv;*.asf;*.qt;*.mov;*.mpg;*.avi;*.wmv;*.mp3;*.wav;*.ra;*.rm;*.wma;*.aif;*.m4a"/>
		<filter name="image_files" ext="*.jpg;*.jpeg;*.gif;*.png"/>
	</filters>
</root>