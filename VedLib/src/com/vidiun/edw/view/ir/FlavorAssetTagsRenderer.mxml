<?xml version="1.0" encoding="utf-8"?>
<FlavorAssetRendererBase xmlns="com.vidiun.edw.view.ir.*" xmlns:mx="http://www.adobe.com/2006/mxml" width="100%"
						 height="100%" >
	<mx:Script>
		<![CDATA[
			import com.vidiun.edw.vo.FlavorAssetWithParamsVO;
			import com.vidiun.vo.VidiunFlavorAsset;
			
			override public function set data(value:Object):void {
				super.data = value;
				var vfa:VidiunFlavorAsset = (value as FlavorAssetWithParamsVO).vidiunFlavorAssetWithParams.flavorAsset;
				if (vfa != null){
					tagsLabel.text = vfa.tags;
					tagsLabel.toolTip = vfa.tags;
				} else {
					tagsLabel.text = "-";
				}
			}
		]]>
	</mx:Script>
	<mx:Label id="tagsLabel" width="100%" textAlign="center" selectable="true"/>
</FlavorAssetRendererBase>
